#!/usr/bin/env bash
# bin/compile <build-dir> <cache-dir>
set -e
set -o pipefail

BUILD_DIR=$1
CACHE_DIR=$2

function indent() {
  c='s/^/ /'
  case $(uname) in
    Darwin) sed -l "$c";;
    *) sed -u "$c";;
  esac
}

pwd
cd $BUILD_DIR
pwd

